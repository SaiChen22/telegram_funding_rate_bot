{
  "version": 2,
  "builds": [
    {
      "src": "api/webhook.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/webhook",
      "dest": "/api/webhook.py"
    }
  ],
  "env": {
    "TELEGRAM_BOT_TOKEN": "@telegram_bot_token",
    "TELEGRAM_CHAT_ID": "@telegram_chat_id",
    "UPPER_THRESHOLD": "@upper_threshold",
    "LOWER_THRESHOLD": "@lower_threshold",
    "CHECK_INTERVAL": "@check_interval"
  }
}